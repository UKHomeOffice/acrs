{
  "appName": "Afghan Citizens Resettlement Scheme",
  "theme": "govUK",
  "behaviours": [
    "hof/components/clear-session",
    "hof/components/session-timeout-warning"
  ],
  "build": {
    "watch": {
      "ignore": [
        "data"
      ]
    }
  },
  "translations": "./apps/acrs/translations",
  "routes": [
    "./apps/acrs",
    "./apps/verify"
  ],
  "session": {
    "name": "acrs.hof.sid"
  },
  "getAccessibility": false,
  "emailerFallback": true
}
